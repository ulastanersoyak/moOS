CompileFlags:
  Add:
    - "-g"
    - "-ffreestanding"
    - "-falign-jumps"
    - "-falign-functions"
    - "-falign-labels"
    - "-falign-loops"
    - "-fstrength-reduce"
    - "-fomit-frame-pointer"
    - "-finline-functions"
    - "-Wno-unused-function"
    - "-fno-builtin"
    - "-Wno-unused-label"
    - "-Wno-cpp"
    - "-Wno-unused-parameter"
    - "-nostdlib"
    - "-nostartfiles"
    - "-nodefaultlibs"
    - "-O0"
    - "-Iinc"
    - "-m32"
